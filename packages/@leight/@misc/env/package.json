    "dependencies": {
        "@leight/utils": "^0.6.4"
    },
    "devDependencies": {
        "@leight/esbuild": "^0.6.2",
        "@leight/eslint-config-eslint": "^0.6.2",
        "@leight/tsconfig": "^0.6.2",
        "@types/node": "^20.4.2",
        "concurrently": "^8.2.0",
        "del-cli": "^5.0.0",
        "eslint": "^8.45.0",
        "typescript": "^5.1.6"
    },
    "bugs": {
        "url": "https://github.com/leight-core/viv/issues"
    },
    "homepage": "https://github.com/leight-core/viv#readme"
