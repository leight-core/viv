{
    "version": "0.5.1",
    "name": "@leight/react-cmdk",
    "description": "Quite cool command pallete for react.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/leight-core/viv.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "type": "module",
    "module": "src/index.ts",
    "types": "src/index.ts",
    "publishConfig": {
        "module": "lib/index.js",
        "types": "lib/index.d.ts"
    },
    "files": [
        "lib",
        "src"
    ],
    "scripts": {
        "build": "tsc --outDir lib && npm run build-tailwind",
        "build-tailwind": "dotenv -v NODE_ENV=production -- npx tailwindcss -i tailwind.css -o ./lib/cmdk.css --postcss --minify",
        "depcheck:disabled": "depcheck",
        "cleanup": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@headlessui/react": "^1.6.4",
        "@heroicons/react": "^2.0.13",
        "html-webpack-plugin": "^5.5.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "reflect-metadata": "^0.1.13"
    },
    "peerDependencies": {
        "react": "^16.x || ^17.x || ^18.x",
        "react-dom": "^16.x || ^17.x || ^18.x"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.18.2",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-react": "^7.17.12",
        "@babel/preset-typescript": "^7.17.12",
        "@leight/eslint-config-eslint": "workspace:^0.5.6",
        "@leight/tsconfig": "workspace:^0.5.16",
        "@types/node": "^18.13.0",
        "@types/react": "^18.0.11",
        "@types/react-dom": "^18.0.5",
        "@types/react-outside-click-handler": "^1.3.1",
        "@types/webpack": "^5.28.0",
        "autoprefixer": "^10.4.7",
        "babel-loader": "^8.2.5",
        "del-cli": "^5.0.0",
        "depcheck": "^1.4.3",
        "dotenv-cli": "^7.0.0",
        "fork-ts-checker-webpack-plugin": "^7.2.11",
        "postcss": "^8.4.14",
        "postcss-prefix-selector": "^1.15.0",
        "tailwindcss": "^3.0.24",
        "ts-loader": "^9.3.0",
        "typescript": "^4.7.3",
        "webpack": "^5.73.0"
    },
    "babel": {
        "presets": [
            "@babel/env",
            "@babel/typescript",
            "@babel/react"
        ]
    }
}
