{
    "version": "0.5.18",
    "name": "@leight/react-cmdk",
    "description": "Quite cool command pallete for react.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/leight-core/viv.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "type": "module",
    "module": "src/index.ts",
    "types": "src/index.ts",
    "publishConfig": {
        "module": "lib/index.js",
        "types": "lib/index.d.ts"
    },
    "files": [
        "lib",
        "src"
    ],
    "scripts": {
        "build": "tsc --outDir lib && npm run build-tailwind",
        "build-tailwind": "dotenv -v NODE_ENV=production -- npx tailwindcss -i tailwind.css -o ./lib/cmdk.css --postcss --minify",
        "depcheck:disabled": "depcheck",
        "cleanup": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.10",
        "@leight/context-client": "workspace:^0.5.24",
        "@leight/i18n": "workspace:^0.5.19",
        "@leight/utils": "workspace:^0.5.21",
        "@leight/utils-client": "workspace:^0.5.22",
        "@mantine/hooks": "^5.10.3",
        "html-webpack-plugin": "^5.5.0",
        "next": "^13.1.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "reflect-metadata": "^0.1.13",
        "tabler-icons-react": "^1.56.0",
        "zustand": "^4.3.3"
    },
    "peerDependencies": {
        "react": "^16.x || ^17.x || ^18.x",
        "react-dom": "^16.x || ^17.x || ^18.x"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@leight/eslint-config-eslint": "workspace:^0.5.8",
        "@leight/tsconfig": "workspace:^0.5.18",
        "@types/node": "^18.13.0",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.10",
        "@types/react-outside-click-handler": "^1.3.1",
        "@types/webpack": "^5.28.0",
        "autoprefixer": "^10.4.13",
        "babel-loader": "^9.1.2",
        "del-cli": "^5.0.0",
        "depcheck": "^1.4.3",
        "dotenv-cli": "^7.0.0",
        "eslint": "^8.34.0",
        "fork-ts-checker-webpack-plugin": "^7.3.0",
        "postcss": "^8.4.21",
        "postcss-prefix-selector": "^1.16.0",
        "tailwindcss": "^3.2.6",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.5",
        "webpack": "^5.75.0"
    },
    "babel": {
        "presets": [
            "@babel/env",
            "@babel/typescript",
            "@babel/react"
        ]
    }
}
