{
    "version": "0.5.5",
    "name": "@leight/generator-server",
    "description": "Set of tools used for source code generation from... source code :).",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/leight-core/viv.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "type": "module",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib",
        "src"
    ],
    "scripts": {
        "init": "npm run build",
        "build": "concurrently npm:build:*",
        "build:esbuild": "node esbuild.mjs",
        "build:types": "tsc --outDir lib",
        "depcheck": "depcheck",
        "lint": "eslint src",
        "cleanup": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@leight/generator": "^0.5.4",
        "@leight/utils": "^0.5.51",
        "@leight/utils-server": "^0.5.48"
    },
    "devDependencies": {
        "@leight/esbuild": "^0.5.28",
        "@leight/eslint-config-eslint": "^0.5.37",
        "@leight/tsconfig": "^0.5.51",
        "@types/node": "^18.15.0",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "depcheck": "^1.4.3",
        "eslint": "^8.36.0",
        "typescript": "^5.0.2"
    },
    "bugs": {
        "url": "https://github.com/leight-core/viv/issues"
    },
    "homepage": "https://github.com/leight-core/viv#readme"
}
