{
	"version": "0.8.0",
	"name": "@leight-core/api",
	"description": "Package with all interfaces used in the whole @leight-core.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/leight-core/api.git"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"sideEffects": false,
	"type": "module",
	"module": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"build": "tsc -p tsconfig.json --outDir lib",
		"dev:watch": "tsc -w -p tsconfig.json --outDir lib --preserveWatchOutput",
		"depcheck": "npx depcheck",
		"lint": "eslint src",
		"cleanup": "pnpm del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
	},
	"dependencies": {
		"@prisma/client": "^4.9.0",
		"@tanstack/react-query": "^4.24.4",
		"antd": "^5.1.7",
		"antd-mobile": "^5.28.0",
		"axios": "^1.3.1",
		"i18next": "^22.4.9",
		"next": "^13.1.6",
		"next-auth": "^4.19.2",
		"p-queue": "^7.3.0",
		"rc-field-form": "^1.27.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"reflect-metadata": "^0.1.13",
		"winston": "^3.8.2"
	},
	"devDependencies": {
		"@leight/eslint-config-eslint": "workspace:^0.0.5",
		"@leight/tsconfig": "workspace:^0.5.10",
		"@types/async-lock": "^1.3.0",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.27",
		"del-cli": "^5.0.0",
		"eslint": "^8.33.0",
		"typescript": "^4.9.5"
	}
}
