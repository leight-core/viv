{
    "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
    "rushVersion": "5.81.0",
    "pnpmVersion": "6.34.0",
    "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0 || >=16.13.0 <17.0.0",
    "projectFolderMinDepth": 2,
    "projectFolderMaxDepth": 2,
    "telemetryEnabled": false,
    "ensureConsistentVersions": true,
    "repository": {
        "url": "git://github.com/leight-core/viv.git"
    },
    "pnpmOptions": {
        "strictPeerDependencies": true
    },
    "projects": [
        {
            "packageName": "@leight/antd",
            "projectFolder": "packages/antd",
            "shouldPublish": true
        },
        {
            "packageName": "@leight/ui",
            "projectFolder": "packages/ui",
            "shouldPublish": true
        },
        {
            "packageName": "@leight/shared",
            "projectFolder": "packages/shared",
            "shouldPublish": true
        },
        {
            "packageName": "@leight/utils",
            "projectFolder": "packages/utils",
            "shouldPublish": true
        },
        {
            "packageName": "@leight/desktop",
            "projectFolder": "packages/desktop",
            "shouldPublish": true
        },
        {
            "packageName": "@leight/mobile",
            "projectFolder": "packages/mobile",
            "shouldPublish": true
        },
        {
            "packageName": "@leight/server",
            "projectFolder": "packages/server",
            "shouldPublish": true
        }
    ]
}
